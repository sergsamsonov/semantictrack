<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bugtrack.entity.lemma" table="lemmas">
        <id column="lemma" name="lemma" type="java.lang.String">
            <generator class="assigned"/>
        </id>
        <property column="ticknum" name="ticknum" type="java.lang.Integer"/>
    </class>
</hibernate-mapping>