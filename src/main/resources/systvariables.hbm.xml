<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bugtrack.entity.systvar" table="systvariables">
        <id column="code" name="code" type="java.lang.String">
            <generator class="assigned"/>
        </id>
        <property column="description" name="descr" type="java.lang.String"/>
        <property column="value" name="value" type="java.lang.String"/>
    </class>
</hibernate-mapping>